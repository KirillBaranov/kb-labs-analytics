{
  "name": "@kb-labs/analytics-contracts",
  "version": "0.1.1",
  "description": "Type definitions and contracts for KB Labs Analytics (shared between core and adapters)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ignore-pattern 'dist/**'",
    "lint:fix": "eslint . --fix --ignore-pattern 'dist/**'"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^24.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3"
  }
}
