{
  "name": "@kb-labs/analytics-adapters",
  "version": "0.1.0",
  "description": "KB Labs Analytics - sinks and middleware adapters",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./sinks/fs": {
      "import": "./dist/sinks/fs.js",
      "types": "./dist/sinks/fs.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "author": {
    "name": "Kirill Baranov",
    "url": "https://github.com/kirill-baranov"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kirill-baranov/kb-labs-analytics.git",
    "directory": "packages/analytics-adapters"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run -c ../../vitest.config.ts",
    "test:watch": "vitest -c ../../vitest.config.ts"
  },
  "dependencies": {
    "@kb-labs/analytics-core": "link:../analytics-core",
    "@aws-sdk/client-s3": "^3.700.0",
    "better-sqlite3": "^11.7.0"
  },
  "devDependencies": {
    "@kb-labs/devkit": "link:../../../kb-labs-devkit",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^24.7.0",
    "rimraf": "^6",
    "tsup": "^8",
    "typescript": "^5",
    "vitest": "^3"
  }
}

